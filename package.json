    "@zendeskgarden/react-loaders": "^8.24.2",
    "@zendeskgarden/react-theming": "^8.24.2",
    "@zendeskgarden/react-typography": "^8.24.2",
    "@zendeskgarden/svg-icons": "^6.24.0",
    "airtable": "0.9.0",
    "axios": "^0.20.0",
    "fuse.js": "^6.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.0"
