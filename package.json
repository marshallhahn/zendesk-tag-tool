{
  "name": "zendesk-tag-tool",
  "version": "1.0.0",
  "description": "Zendesk app to help agents quickly search through and apply tags from a pre-defined list on Airtable.",
  "keywords": [
    "zendesk",
    "tag",
    "tool"
  ],
  "author": "@marshallhahn",
  "license": "BSD-4-Clause",
  "engines": {
    "node": ">=6.11.5"
  },
  "scripts": {
    "start": "zcli apps:server ./dist",
    "watch": "webpack --watch --mode development",
    "test": "jest",
    "build:dev": "webpack --mode development",
    "build": "webpack --mode production",
    "validate": "zcli apps:validate ./dist",
    "create": "zcli apps:create ./dist",
    "update": "zcli apps:update ./dist",
    "package": "zcli apps:package ./dist",
    "clean": "zcli apps:clean ./dist",
    "bump-major": "zcli apps:bump -M .",
    "bump-minor": "zcli apps:bump -m .",
    "bump-patch": "zcli apps:bump -p ."
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
